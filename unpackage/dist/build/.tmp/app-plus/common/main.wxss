/* 一像素 */
.border-1px {
	position: relative;
}
.border-1px::before {
	content: '';
	width: 100%;
	display: block;
	position: relative;
	left: 0;
	bottom: 0;
	border-bottom: 1px solid #d9d9d9;
}
@media (min-device-pixel-ratio: 1.5), (-webkit-min-device-pixel-ratio: 1.5) {
.border-1px::before {
		-ms-transform: scaleY(0.67);
		    transform: scaleY(0.67);
		-webkit-transform: scaleY(0.67);
}
}
@media (min-device-pixel-ratio: 2), (-webkit-min-device-pixel-ratio: 2) {
.border-1px::before {
		-ms-transform: scaleY(0.5);
		    transform: scaleY(0.5);
		-webkit-transform: scaleY(0.5);
}
}
@media (min-device-pixel-ratio: 3), (-webkit-min-device-pixel-ratio: 3) {
.border-1px::before {
		-ms-transform: scaleY(0.5);
		    transform: scaleY(0.5);
		-webkit-transform: scaleY(0.5);
}
}
